
  <div class="input-container">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name, autofocus: true %>
  </div>
  <div class="input-container">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name, autofocus: true %>
  </div>
  <div class="input-container">
    <%= f.label :startDate %><br />
    <%= f.date_field :start_date, autofocus: true %>
  </div>

  <div class="input-container">
    <%= f.label :startDate %><br />
    <%= f.date_field :end_date, autofocus: true %>
  </div>
  
  <div class="input-container">
    <%= f.label :monthly_charge %><br />
    <%= f.number_field :monthly_charge, autofocus: true %>
  </div>
  <div class="input-container">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="custom-select">
     <%#= f.label :company_name %>
     <%= f.collection_select :company_id, Company.order(:id), :id, :name %>
  </div>

   <div class="custom-select">
    <%#= f.label :Role %>
    <%= f.select :user_role, User::USER_ROLES.keys.to_a %>
  </div>
  <div class="custom-select">
    <%#= f.label :active %>
    <%= f.check_box :is_active %>
  </div>
  <%= f.fields_for :image, @image do |field|%>
  <div class="input-container">
    <%= field.label :photo %>
    <%= field.file_field :photo %>
  </div>
  <% end %>

  <div class="input-container">
    <%= f.label :notes %>
    <%= f.rich_text_area :notes %>
  </div>
 <!--  -->
  <div class="send-container">
    <%= f.submit "Update" %>
  </div>
<%= render 'partials/js_partial' %>



