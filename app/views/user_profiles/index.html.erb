
<h1>User</h1>

<table class="zui-table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last name</th>
      <th>Active</th>
      <th>Email</th>
      <th>show</th>
      <th>edit</th>
      <th>delete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @users.each do |user_profile| %>
      <tr>
        <td><%= user_profile.first_name %></td>
        <td><%= user_profile.last_name %></td>
        <td><%= user_profile.is_active %></td>
        <td><%= user_profile.email %></td>
        <td><%= link_to 'Show', user_profile_path(user_profile) %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
        <td><%= link_to 'Destroy', user_profile_path(user_profile), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

